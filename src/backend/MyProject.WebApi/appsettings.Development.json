{
  "Serilog": {
    "Using": ["Serilog.Sinks.Seq"],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:{INIT_SEQ_PORT}"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "Database": "Host=localhost;Port={INIT_DB_PORT};Database={INIT_PROJECT_SLUG};Username=admin;Password=pwd"
  },
  "Caching": {
    "Enabled": true,
    "DefaultExpiration": "00:10:00"
  },
  "RateLimiting": {
    "Global": {
      "PermitLimit": 300,
      "Window": "00:00:10"
    },
    "Registration": {
      "PermitLimit": 50,
      "Window": "00:00:10"
    },
    "Auth": {
      "PermitLimit": 300,
      "Window": "00:00:10"
    },
    "Sensitive": {
      "PermitLimit": 300,
      "Window": "00:00:10"
    },
    "AdminMutations": {
      "PermitLimit": 300,
      "Window": "00:00:10"
    }
  },
  "Authentication": {
    "Jwt": {
      "AccessTokenLifetime": "01:40:00"
    }
  },
  "Email": {
    "FrontendBaseUrl": "http://localhost:{INIT_FRONTEND_PORT}"
  },
  "Hosting": {
    "ForceHttps": false
  },
  "Captcha": {
    "SecretKey": "1x0000000000000000000000000000000AA"
  },
  "FileStorage": {
    "Endpoint": "http://localhost:{INIT_STORAGE_PORT}",
    "AccessKey": "minioadmin",
    "SecretKey": "minioadmin",
    "BucketName": "{INIT_PROJECT_SLUG}-files",
    "UseSSL": false
  },
  "Cors": {
    "AllowAllOrigins": true,
    "AllowedOrigins": [],
    "PolicyName": "DefaultCorsPolicy"
  }
}
